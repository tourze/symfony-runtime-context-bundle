{
  "name": "tourze/symfony-runtime-context-bundle",
  "description": "Symfony 上下文管理和延迟执行模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "monolog/monolog": "^3.1",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^7.3",
    "symfony/console": "^7.3",
    "symfony/dependency-injection": "^7.3",
    "symfony/event-dispatcher": "^7.3",
    "symfony/framework-bundle": "^7.3",
    "symfony/http-kernel": "^7.3",
    "symfony/service-contracts": "^3.6",
    "symfony/yaml": "^7.3",
    "tourze/backtrace-helper": "0.1.*",
    "tourze/bundle-dependency": "1.*",
    "tourze/symfony-dependency-service-loader": "1.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "0.0.*",
    "tourze/phpunit-symfony-unit-test": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\RuntimeContextBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\RuntimeContextBundle\\Tests\\": "tests"
    }
  }
}