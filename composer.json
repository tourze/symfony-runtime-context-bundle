{
  "name": "tourze/symfony-runtime-context-bundle",
  "description": "上下文管理模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/console": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/config": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/service-contracts": "^2.5 || ^3"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\Symfony\\RuntimeContextBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\Symfony\\RuntimeContextBundle\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1"
  }
}